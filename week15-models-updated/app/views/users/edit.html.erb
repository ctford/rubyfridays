<h1> Editing a user</h1>
<%= form_for :user, url: user_path(@user.id), method: :put do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %>
    <%= f.label :age %>
    <%= f.text_field :age %>
    <%= f.submit "Save this!" %>
<% end %>
